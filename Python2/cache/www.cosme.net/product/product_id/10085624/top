<script type="text/javascript">
var bottom_google_num_ads = 0;
function google_ad_request_done(data) {
    if (data.length == 0 || data[0].type != 'text') { return; }

    var str = '';
    bottom_google_num_ads += data.length;
    for (var i=0; i<data.length; i++) {
        var e = data[i];
        str += '<li>'
            +  '<a href="'+ e.url+'" target="_blank" class="adtitle">' + e.line1 + '</a><br />'
            +  '<span class="msg">'+e.line2+' '+e.line3+'</span><br />'
            +  '<p class="url"><a href="'+ e.url+'" target="_blank">' + e.visible_url+'</a></p>'
            +  '</li>';
    }

    document.write('<div id="main-adsense">'
                   + '<p><a href="' + google_info.feedback_url + '" target="_blank">Ads by Google</a></p>'
                   + (data.length < 3 ? '<ul class="ads-l">' : '<ul class="ads">') + str + '</ul></div>');
}
google_ad_client = 'ca-istyle-bottom_js';
google_ad_channel = 'product_top_bottom';
google_ad_output = 'js';
google_max_num_ads = '3';
google_ad_type = 'text';
google_language = 'ja';
google_encoding = 'sjis';
google_safe = 'high';
google_feedback = 'on';
if(typeof(google_num_ads) != 'undefined'){
    google_skip = google_num_ads;
}
</script>
