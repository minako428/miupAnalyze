    if (location.href.indexOf(validLocation) !== 0) {
        return;
    }
    var aid = "1";
    var vntg_src = "//tag.dl.vntg.jp/onetag?aid=" + aid + "&media_url=" + encodeURIComponent(location.href);
    var vntg_script = '<scr'+'ipt type="text/javascript" src="'+vntg_src+'"></scr'+'ipt>';
    document.write(vntg_script);
})();
