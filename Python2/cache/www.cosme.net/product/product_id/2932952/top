var google_num_ads = 0;
function google_ad_request_done(data) {
    if (data.length == 0 || data[0].type != 'text') { return; }

    var str = '';
    google_num_ads += data.length;
    for (var i=0; i<data.length; i++) {
        var e = data[i];
        str += '<li>'
            +  '<a href="'+ e.url+'" class="title" target="_blank">'
            +       e.line1+'</a><br />'
            +  '<span class="msg">'+e.line2+'<br />'+e.line3+'</span><br />'
            +  '<a href="'+ e.url+'" class="url" target="_blank">'
            +       e.visible_url+'</a>'
            +  '</li>';
    }

    document.write('<div id="sub-adsense">'
                   + '<p><a href="' + google_info.feedback_url
                   +    '" target="_blank">Ads by Google</a></p>'
                   + (data.length == 1 ? '<ul class="ads-l">'
                                       : '<ul class="ads">')
                   + str
                   +'</ul></div>');
}
google_ad_client = 'ca-istyle_js';
google_ad_channel = 'product';
google_ad_output = 'js';
google_max_num_ads = '4';
google_ad_type = 'text';
google_language = 'ja';
google_encoding = 'sjis';
google_safe = 'high';
google_feedback = 'on';
if(typeof(bottom_google_num_ads) != 'undefined'){
    google_skip = bottom_google_num_ads;
}
